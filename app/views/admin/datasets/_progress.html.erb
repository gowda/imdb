<div
  class="row align-items-center dataset-progress"
  data-name="<%= name %>"
  style="display: none;"
>
  <div class="col-10">
    <div class="progress">
      <div
        class="progress-bar progress-bar-striped progress-bar-animated"
        role="progressbar"
        aria-valuenow="0"
        aria-valuemin="0"
        aria-valuemax="100"
        style="width: 0%"
      >
      </div>
    </div>
  </div>
  <div class="col-2">
    <span class="label">0%</span>
  </div>
</div>
